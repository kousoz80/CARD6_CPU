            Xfd[i].in = null;
              }
          if( Xfd[i].out != null ){
            Xfd[i].out.close();
            Xfd[i].out = null;
              }
           }
      } catch( Exception e){ assertError(&quot;i/o error&quot;); }
      }
   }
  return DONE;
}


